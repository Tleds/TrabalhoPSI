@model PUC.LDSI.Domain.Entities.Avaliacao
@{
    ViewData["Title"] = "Exclusão de Avaliacao";
}
<h3>Confirma a exclusão dessa turma?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Descricao):
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Descricao)
        </dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-default" />&nbsp;
        <a asp-action="Index" class="btn btn-default">Cancelar</a>
    </form>
</div>

